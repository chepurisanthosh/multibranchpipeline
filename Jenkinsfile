pipeline{
    
    agent any
    

    
    stages{
        
        stage('Build Code')
        {
            steps{
                bat 'echo "Build Code"'
            }
        }
        
        stage('Release Code'){
            steps{
            
                   
                        echo "relase the code to artifactory"
                
               } 
               
            }
                
            }
        }
   
